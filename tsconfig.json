{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "rootDir": "src",
    "declaration": false,
    "sourceMap": false,
    "removeComments": true,
    "emitDeclarationOnly": false,
    // "allowImportingTsExtensions": true,
    "noEmit": false,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
